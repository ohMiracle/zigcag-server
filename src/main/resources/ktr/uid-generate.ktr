        <jsScript_name>Script 1</jsScript_name>
        <jsScript_script>//Script here

var uid = uid+'' ;
var state=0
var uids = uid.split('')

var temp=0
var d=true
var e=true
var f=true
var g=true
for (var i = 0; i &lt; uids.length; i++) {
  var id = uids[i]
  if (i > 0) {
    if (d) {
      d = Number(temp) == id
    }
    if (e) {
      e = Number(temp) + 1 == id
    }
    if (f) {
      f = Number(temp) - 1 == id
    }
	if(g){
	  g = id == '0'
	}
    if (!(d | e | f|g)) {
      break
    }
  }
  temp = id
}

if (d | e | f|g) {
  state = 1
}
</jsScript_script>
